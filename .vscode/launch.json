    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch GRPC",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build rpc",
            "program": "${workspaceFolder}/src/AspNet5.GoodPracticies.Grpc/bin/Debug/net5.0/AspNet5.GoodPracticies.Grpc.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/AspNet5.GoodPracticies.Grpc",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Launch API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build api",
            "program": "${workspaceFolder}/src/AspNet5.GoodPracticies.Api/bin/Debug/net5.0/AspNet5.GoodPracticies.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/AspNet5.GoodPracticies.Api",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
    ]
}